{
  "name": "elect-tool",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "electron/main.cjs",
  "scripts": {
    "dev": "vite",
    "dev:electron": "node scripts/dev-electron.cjs",
    "dev:app": "concurrently \"vite\" \"node scripts/dev-electron.cjs\"",
    "build": "vite build",
    "preview": "vite preview",
    "dist": "vite build && electron-builder",
    "server": "node server/index.cjs"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.2",
    "better-sqlite3": "^9.4.0",
    "cors": "^2.8.5",
    "element-plus": "^2.11.8",
    "express": "^4.19.2",
    "pinia": "^2.2.0",
    "vue": "^3.4.0",
    "vue-i18n": "^9.0.0",
    "vue-router": "^4.4.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.6.0",
    "@vue/tsconfig": "^0.8.0",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "electron": "^30.0.0",
    "electron-builder": "^24.6.5",
    "typescript": "^5.0.0",
    "vite": "^4.5.0",
    "wait-on": "^7.2.0"
  },
  "build": {
    "appId": "com.elect.tool",
    "files": [
      "dist/**",
      "electron/**"
    ],
    "asarUnpack": [
      "**/*.node"
    ],
    "win": {
      "target": "nsis"
    }
  }
}
