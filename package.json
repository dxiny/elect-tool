{
  "name": "elect-tool",
  "private": true,
  "version": "1.1.7",
  "author": "ElectTool",
  "description": "初始化项目",
  "productName": "ElectTool",
  "type": "module",
  "main": "electron/main.cjs",
  "scripts": {
    "dev": "vite",
    "dev:electron": "node scripts/dev-electron.cjs",
    "dev:app": "concurrently \"vite\" \"node scripts/dev-electron.cjs\"",
    "build": "cross-env NODE_OPTIONS=--max-old-space-size=4096 vite build",
    "preview": "vite preview",
    "dist": "npm run build && electron-builder",
    "server": "node server/index.cjs"
  },
  "dependencies": {
    "@ant-design/icons-vue": "^7.0.1",
    "@types/three": "^0.182.0",
    "ant-design-vue": "^4.2.6",
    "better-sqlite3": "^9.6.0",
    "cors": "^2.8.5",
    "earth-flyline": "^1.6.4",
    "echarts": "^6.0.0",
    "express": "^4.22.1",
    "pinia": "^2.2.0",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "three": "^0.182.0",
    "vue": "^3.4.0",
    "vue-i18n": "^9.0.0",
    "vue-router": "^4.4.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.50.0",
    "@typescript-eslint/parser": "^8.50.0",
    "@vitejs/plugin-vue": "^4.6.0",
    "@vue/tsconfig": "^0.8.0",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "electron": "^30.0.0",
    "electron-builder": "^24.6.5",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-vue": "^10.6.2",
    "prettier": "^3.7.4",
    "typescript": "^5.0.0",
    "vite": "^4.5.0",
    "wait-on": "^7.2.0"
  },
  "build": {
    "appId": "com.elect.tool",
    "productName": "ElectTool",
    "directories": {
      "output": "dist_electron",
      "buildResources": "build"
    },
    "files": [
      "dist/**",
      "electron/**"
    ],
    "asarUnpack": [
      "**/*.node"
    ],
    "win": {
      "target": "nsis",
      "icon": "build/iconWin.ico"
    },
    "mac": {
      "target": "dmg",
      "icon": "build/iconMac.icns"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "shortcutName": "ElectTool"
    }
  }
}
